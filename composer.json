{
    "name": "jamal13647850/sms-api",
    "description": "A unified PHP library for sending SMS messages through various Iranian SMS providers",
    "type": "library",
    "keywords": [
        "sms",
        "api",
        "farazsms",
        "smsir",
        "payamito",
        "farapayamak",
        "elanak",
        "medianasms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sayyed Jamal Ghasemi",
            "email": "jamal13647850@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "jamal13647850\\smsapi\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-soap": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "vlucas/phpdotenv": "For loading configuration from .env files"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
