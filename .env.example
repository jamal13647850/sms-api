# SMS API Library Configuration
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env files with real credentials!

# ============================================
# FarazSMS Configuration
# https://ippanel.com
# ============================================
FARAZSMS_USERNAME=your_faraz_username
FARAZSMS_PASSWORD=your_faraz_password
FARAZSMS_FROM=3000XXXXX

# ============================================
# SMS.ir Configuration
# https://sms.ir
# ============================================
SMSIR_API_KEY=your_smsir_api_key
SMSIR_FROM=3000XXXXX

# ============================================
# FaraPayamak Configuration
# https://farapayamak.com
# ============================================
FARAPAYAMAK_USERNAME=your_farapayamak_username
FARAPAYAMAK_PASSWORD=your_farapayamak_password
FARAPAYAMAK_FROM=1000XXXXX

# ============================================
# Payamito Configuration
# https://payamito.com
# ============================================
PAYAMITO_USERNAME=your_payamito_username
PAYAMITO_PASSWORD=your_payamito_password
PAYAMITO_FROM=1000XXXXX

# ============================================
# MedianaSMS Configuration
# https://medianasms.com
# ============================================
MEDIANASMS_USERNAME=your_mediana_username
MEDIANASMS_PASSWORD=your_mediana_password
MEDIANASMS_FROM=3000XXXXX

# ============================================
# Elanak Configuration
# http://elanak.ir
# ============================================
ELANAK_USERNAME=your_elanak_username
ELANAK_PASSWORD=your_elanak_password
ELANAK_FROM=1000XXXXX

# ============================================
# Melipayamak Configuration
# https://melipayamak.ir
# ============================================
# IMPORTANT: MELIPAYAMAK_PASSWORD is the ApiKey from panel settings
# (found under "توسعه‌دهندگان" menu), NOT your account password
MELIPAYAMAK_USERNAME=your_melipayamak_username
MELIPAYAMAK_PASSWORD=your_melipayamak_apikey
MELIPAYAMAK_FROM_PRIMARY=5000XXXXXXXX
MELIPAYAMAK_FROM_SECONDARY=5000XXXXXXXX

# ============================================
# Test Configuration
# ============================================
# Test recipient phone numbers (for integration tests)
TEST_RECIPIENT_1=0912XXXXXXX
TEST_RECIPIENT_2=0939XXXXXXX
